<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".backstage.BackstageAccountManageMainFragment">

    <?xml version="1.0" encoding="utf-8"?>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_backstage_account_manage_main_user"
        android:layout_width="55dp"
        android:layout_height="42dp"
        android:text="@string/btn_backstage_account_manage_main_user"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_backstage_account_manage_main_check"
        android:layout_width="55dp"
        android:layout_height="42dp"
        android:text="@string/btn_backstage_account_manage_main_check"
        app:layout_constraintBottom_toBottomOf="@+id/btn_backstage_account_manage_main_user"
        app:layout_constraintStart_toEndOf="@+id/btn_backstage_account_manage_main_user"
        app:layout_constraintTop_toTopOf="@+id/btn_backstage_account_manage_main_user" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_backstage_account_manage_main_suspend"
        android:layout_width="55dp"
        android:layout_height="42dp"
        android:text="@string/btn_backstage_account_manage_main_suspend"
        app:layout_constraintBottom_toBottomOf="@+id/btn_backstage_account_manage_main_check"
        app:layout_constraintStart_toEndOf="@+id/btn_backstage_account_manage_main_check"
        app:layout_constraintTop_toTopOf="@+id/btn_backstage_account_manage_main_check" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_backstage_account_manage_main_service"
        android:layout_width="55dp"
        android:layout_height="42dp"
        android:text="@string/btn_backstage_account_manage_main_service"
        app:layout_constraintBottom_toBottomOf="@+id/btn_backstage_account_manage_main_suspend"
        app:layout_constraintStart_toEndOf="@+id/btn_backstage_account_manage_main_suspend"
        app:layout_constraintTop_toTopOf="@+id/btn_backstage_account_manage_main_suspend" />

    <LinearLayout
        android:id="@+id/ll_backstage_account_manage_main_base"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="36dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_backstage_account_manage_main_user">

        <LinearLayout
            android:id="@+id/ll_backstage_account_manage_main_searchbar"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.SearchView
                android:id="@+id/sv_backstage_complaint_manage"
                android:layout_width="235dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical"
                app:iconifiedByDefault="false" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_backstage_account_manage_main_add"
                android:layout_width="54dp"
                android:layout_height="38dp"
                android:layout_marginStart="40dp"
                android:text="@string/btn_backstage_account_manage_main_add" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_backstage_account_manage_main_title"
            android:layout_width="match_parent"
            android:layout_height="24dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_backstage_account_manage_main_email_title"
                android:layout_width="84dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tv_backstage_account_manage_main_email_title"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/tv_backstage_account_manage_main_name_title"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tv_backstage_account_manage_main_name_title"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/tv_backstage_account_manage_main_time_title"
                android:layout_width="49dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/tv_backstage_account_manage_main_time_title"
                android:textColor="#000000" />
        </LinearLayout>

        <TextView
            android:id="@+id/tv_backstage_account_manage_main_no_data"
            android:layout_width="64dp"
            android:layout_height="24dp"
            android:layout_gravity="center_horizontal"
            android:text="@string/tv_backstage_account_manage_main_no_data" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_backstage_account_manage_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>