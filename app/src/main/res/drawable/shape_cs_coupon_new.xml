<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <shape
            android:shape="rectangle">
            <corners android:radius="8dp"/>
            <solid
                    android:color="#FE0404"  />
        </shape>
    </item>

    <item  >
        <rotate
            android:fromDegrees="45"
            android:toDegrees="-135"
            android:pivotX="-15%"
            android:pivotY="60%" >
            <shape
                android:shape="rectangle"   >
                <solid
                    android:color="#FFFFFF"  />
            </shape>

        </rotate>
    </item>

    <item  >
        <rotate
            android:fromDegrees="45"
            android:toDegrees="-135"
            android:pivotX="10%"
            android:pivotY="0%" >
            <shape
                android:shape="rectangle"   >
                <solid
                    android:color="#FFFFFF"  />
            </shape>

        </rotate>
    </item>

</layer-list>